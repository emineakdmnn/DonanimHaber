+
+
+abstract class BaseActivity<DB: ViewDataBinding, VM: ViewModel>: AppCompatActivity(){
+    lateinit var dataBinding: DB
+    lateinit var viewModel: VM
+
+    @LayoutRes
+    abstract fun getLayoutsRes(): Int
+
